#######################################################
# Product service codes (subset; all included)
#######################################################
ex:ServiceCodes a skos:ConceptScheme ; skos:prefLabel "Piping product service codes" .
ex:SC_AI a skos:Concept ; skos:inScheme ex:ServiceCodes ; skos:prefLabel "AI – Air, instrument" ; skos:notation "AI" .
ex:SC_AP a skos:Concept ; skos:inScheme ex:ServiceCodes ; skos:prefLabel "AP – Air, plant" ; skos:notation "AP" .
ex:SC_PL a skos:Concept ; skos:inScheme ex:ServiceCodes ; skos:prefLabel "PL – Process hydrocarbons liquid" ; skos:notation "PL" .
ex:SC_VF a skos:Concept ; skos:inScheme ex:ServiceCodes ; skos:prefLabel "VF – Vent, flare" ; skos:notation "VF" .
ex:SC_PW a skos:Concept ; skos:inScheme ex:ServiceCodes ; skos:prefLabel "PW – Produced water" ; skos:notation "PW" .
ex:SC_WM a skos:Concept ; skos:inScheme ex:ServiceCodes ; skos:prefLabel "WM – Water, main sea" ; skos:notation "WM" .

#######################################################
# Tag numbering SHACL shapes (equipment, instrument, valve, line numbers)
#######################################################
ex:EquipmentTagShape a sh:NodeShape ;
  sh:targetClass hvens:EquipmentTag ;
  sh:property [
    sh:path hvens:number ;
    sh:datatype xsd:string ;
    sh:pattern "^[0-9]{2}-[A-Z]{2}[0-9]{2}[A-Z]?(\([A-Z0-9]{1,2}\))?$" ] .

ex:InstrumentTagShape a sh:NodeShape ;
  sh:targetClass hvens:InstrumentTag ;
  sh:property [
    sh:path hvens:number ;
    sh:datatype xsd:string ;
    sh:pattern "^[0-9]{2}-[A-Z]{2,5}[0-9]{3,4}[A-Z]$" ] .

ex:ValveTagShape a sh:NodeShape ;
  sh:targetClass hvens:ValveTag ;
  sh:property [
    sh:path hvens:number ;
    sh:datatype xsd:string ;
    sh:pattern "^[0-9]{2}-[0-9]{3,4}[A-Z]{2}$" ] .

ex:LineNumberShape a sh:NodeShape ;
  sh:targetClass hvens:LineNumber ;
  sh:property [
    sh:path hvens:number ;
    sh:datatype xsd:string ;
    sh:pattern "^[0-9]{1,2}(")?-[A-Z]{2}-[0-9]{2}[0-9]{3,4}-[A-Z]{2}[0-9]{2}[A-Z](?:[A-Z])?$" ] .
