PREFIX data: <http://www.webstep.no/workshop/data/>
PREFIX ens:  <http://www.webstep.no/workshop/ens/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

# The dataset after applying the changes in Introduction to SPARQL Protocol and RDF Query Language (SPARQL) section of the workshop
# Created a new System object and assigned the pumps to this system. Removed old literal representing the system.

# Centrifugal pump type (PA)
ens:Pump_PA  a    ens:Pump;
        ens:name  "Centrifugal pump" .

# # Seawater lift pump type (PH) 
ens:Pump_PH  a           ens:Pump;
        ens:description  "A submerged pump with the motor not submerged.";
        ens:name         "Submerged pump" .

# Seawater lift pump instances (50-PH50A and 50-PH50B)
data:50_PH50A  a                    ens:Pump_PH , ens:Pump;
        ens:code                    "50-PH50A";
        ens:designFlow_m3h          1460.0;
        ens:diffPressure_barg       16.0;
        ens:locationDepth           [ ens:hasUom    ens:meter;
                                      ens:hasValue  16.0
                                    ];
        ens:minFlow_m3h             229.0;
        ens:normalFlow_m3h          1200.0;
        ens:operatingPressure_barg  9.0;
        ens:partOfSystem            data:System50;
        ens:power_kW                700.0 .

data:50_PH50B  a                    ens:Pump_PH , ens:Pump;
        ens:code                    "50-PH50B";
        ens:designFlow_m3h          1460.0;
        ens:diffPressure_barg       16.0;
        ens:locationDepth           data:50_PH50B_locationDepth;
        ens:minFlow_m3h             229.0;
        ens:normalFlow_m3h          1200.0;
        ens:operatingPressure_barg  9.0;
        ens:partOfSystem            data:System50;
        ens:power_kW                700.0 .

data:50_PH50B_locationDepth
        ens:hasUom    ens:meter;
        ens:hasValue  16.0 .

# Centrifugal pump individuals (50-PA51A, 50-PA51B, 50-PA52)
data:50_PA51A  a  ens:Pump_PA , ens:Pump .

data:50_PA51B  a  ens:Pump_PA , ens:Pump .

data:50_PA52  a  ens:Pump_PA , ens:Pump .

# System 50
data:System50  a  ens:System;
        rdfs:label "System 50" .