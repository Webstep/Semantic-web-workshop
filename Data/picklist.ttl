@prefix : <http://example.org/traffic#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Red rdf:type owl:NamedIndividual .
:Yellow rdf:type owl:NamedIndividual .
:Green rdf:type owl:NamedIndividual .

:TrafficLightColor rdf:type owl:Class ;
    owl:oneOf ( :Venus :Mars :Earth ) .

:hasTrafficLightColor rdf:type owl:ObjectProperty .

:Intersection rdf:type owl:Class ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty :hasTrafficLightColor ;
        owl:allValuesFrom :TrafficLightColor
    ] .

:Blue rdf:type owl:NamedIndividual .
:myIntersection rdf:type :Intersection ;
    :hasTrafficLightColor :Gaia .

:Blue owl:differentFrom :Red, :Yellow, :Green .